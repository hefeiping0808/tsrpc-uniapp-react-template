{
  "name": "react-admin",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --mode dev",
    "build": "tsc && vite build",
    "build:test": "tsc && vite build --mode test",
    "build:dev": "tsc && vite build --mode dev",
    "build:prod": "tsc && vite build --mode prod",
    "build:github": "tsc && vite build --mode github --base=/React-Admin/",
